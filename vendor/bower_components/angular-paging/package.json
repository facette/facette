  "devDependencies": {
    "angular": ">=1.3.1 <1.6",
    "angular-mocks": ">=1.3.1 <1.6",
    "grunt": "~0.4.5",
    "grunt-contrib-copy": "~0.8.0",
    "grunt-contrib-uglify": "~0.5.0",
    "jasmine-core": "~2.3.4",
    "karma": "~0.13.6",
    "karma-jasmine": "~0.3.6",
